.i32 N 1000

.str TRUE "True\n"
.str FALSE "False\n"

.func main 0 2
    push as f32 1
    push as f32 2
    cmp lt f32
    jump false PRINT_FALSE
label PRINT_TRUE
    push const string TRUE
    syscall Print
    jump CONTINUE
label PRINT_FALSE
    push const string FALSE
    syscall Print
label CONTINUE
    return

