.str nl "\n"
.i32 N 4

.func fact 1 1
    push local word 0
    push as i32 1
    jump gt i32 else
    push as i32 1
    return word
label else
    push local word 0
    dup word
    push as i32 1
    sub i32
    call fact 
    mul i32
    return word

.func main 0 0
    push const word N
    call fact
    cast i32 i64
    syscall PrintI64
    push const string nl
    syscall Print
    return

